generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Testimonial {
  id                  String   @id @default(cuid())
  name                String
  email               String
  intervention_type   String
  message             String
  rating              Int
  about_delivery_lead String
  created_at          DateTime @default(now())
}
